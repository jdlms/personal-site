{
  "extends": "astro/tsconfigs/base",
  "exclude": ["functions/**/*"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "allowJs": true,
    "types": ["@cloudflare/workers-types", "@astrojs/cloudflare"]  }
  
}
